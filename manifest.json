{
	"manifest_version": 3,
	"name": "Canvas To Calendar",
	"description": "Inject dates and assignments from your canvas classes into your google calendar",
	"version": "1",
	"action": {
		"default_popup": "hello.html",
		"default_icon": "C2Cupdated.png"
	},
	"permissions": ["tabs", "scripting", "storage", "identity"],
	"host_permissions": ["*://*/*"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"key": "",
	"oauth2": {
		"client_id": "609099671280-trlqa1rlea2rn5q6gnrm7oilo2v1f8l3.apps.googleusercontent.com",
		"scopes": ["https://www.googleapis.com/auth/calendar"]
	}
}
