{
	"manifest_version": 3,
	"name": "Canvas To Calendar",
	"description": "Inject dates and assignments from your canvas classes into your google calendar",
	"version": "1",
	"action": {
		"default_popup": "hello.html",
		"default_icon": "C2Cupdated.png"
	},
	"content_scripts": [
		{
			"matches": ["*://canvas.ucsc.edu/*"],
			"js": ["scraping.js"]
		}
	],
	"permissions": ["tabs", "scripting"],
	"host_permissions": ["https://canvas.ucsc.edu/*"]
}
